@model IEnumerable<Producto>

@{
    ViewData["Title"] = "Productos";
}

<!-- bradcam_area_start -->
<div class="bradcam_area breadcam_bg overlay">
    <h3>Productos</h3>
</div>
<!-- bradcam_area_end -->
<!-- best_burgers_area_start  -->
<div class="best_burgers_area">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="section_title text-center mb-80">
          <span>Hamburguesas</span>
          <h3>Las Mejores Hamburguesas</h3>
        </div>
      </div>
    </div>
    <div class="row">
      @foreach (var obj in ViewBag.Hamburguesas)
    {
      <div class="col-xl-6 col-md-6 col-lg-6">
        <div class="single_delicious d-flex align-items-center">
          <div class="thumb">
            <img src="/img/products/burger/@obj.Imagen" alt="@obj.Nombre img">
          </div>
          <div class="info">
            <h3>@obj.Nombre</h3>
            <p>@obj.Descripcion</p>
            <span>@obj.Precio€</span>
          </div>
        </div>
      </div>
    }
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="iteam_links">
          <a class="boxed-btn5" href="Menu.html">Más productos</a>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- best_burgers_area_end  -->
